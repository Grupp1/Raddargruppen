<?xml version="1.0" encoding="utf-8"?>

<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:versionCode="1"
	android:versionName="1.0"
	package="raddar.gruppen">
	<uses-sdk android:minSdkVersion="10" />

	<application

		android:icon="@drawable/r"
		android:label="@string/app_name" android:debuggable="true">

	

		<uses-library android:name="com.google.android.maps" />

		<activity
			android:name="raddar.views.StartView"
			android:label="@string/app_name"
			android:theme="@android:style/Theme.NoTitleBar">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity
			android:name="raddar.views.MainView"
			android:label="@string/app_name">

		</activity>

		<activity
			android:name="raddar.views.Map"
			android:label="@string/app_name">

		</activity>
		<activity android:name="raddar.views.InboxView"></activity>
		<activity android:name="raddar.views.ReadMessageView"></activity>

		<activity android:name="raddar.views.MessageChoiceView"></activity>

		<activity android:name="raddar.views.SendMessageView"></activity>
		<activity android:name="raddar.views.ContactView"></activity>

		<activity android:name="raddar.views.ServiceView"></activity>
		<activity android:name="raddar.views.AddContactView"></activity>

	</application>

	<uses-permission android:name="android.permission.INTERNET" />

</manifest>