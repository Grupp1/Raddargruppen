<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <EditText android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/enter_number" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_marginTop="145dp" android:id="@+id/enter_number"></EditText>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/call" android:layout_centerVertical="true" android:layout_centerHorizontal="true" android:text="@string/send_send"></Button>
    
</RelativeLayout>
